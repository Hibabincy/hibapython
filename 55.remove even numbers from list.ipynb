   "source": [
    "#Remove even numbers from list\n",
    "List = [7, 22,33, 36, 56, 61, 72,77,79]\n",
    "print(\"List Items = \", List)\n",
    " \n",
    "for even in List:\n",
    "    if (even % 2 == 0):\n",
    "        List.remove(even)\n",
    "   \n",
    "print(\"List after removing even Numbers = \", List)\n",
    " \n"
   ]
